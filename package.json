{
  "name": "core-auth",
  "type": "module",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "dotenv -e .env -- node --import=extensionless/register ./dist/index.js",
    "dev": "dotenv -e .env.dev -- npx tsx src/index.ts",
    "dev:up": "docker compose -f docker-compose.dev.yml up -d",
    "dev:down": "docker compose -f docker-compose.dev.yml down",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:push:force": "npx prisma db push --accept-data-loss",
    "db:reset": "npx prisma migrate reset",
    "db:format": "npx prisma format",
    "build": "tsc && tsc-alias"
  },
  "author": "rca@sazanami.dev",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@prisma/client": "^6.14.0",
    "bcryptjs": "^3.0.2",
    "chalk": "^5.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv-cli": "^10.0.0",
    "express": "^5.1.0",
    "extensionless": "^1.9.9",
    "nanoid": "^5.1.5",
    "tsc-alias": "^1.8.16",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/chalk": "^0.4.31",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "esbuild": "^0.25.9",
    "prisma": "^6.14.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "volta": {
    "node": "22.18.0"
  }
}
